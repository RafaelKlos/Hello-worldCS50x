#include <stdio.h>

int main(){
    int x;
    int y;
    
    /// compara 2 numeros e mostra relação
    
    printf("Digite o valor de x: ");
    scanf("%i",&x);
    printf("Digite o valor de y: ");
    scanf("%i",&y);
    if(x > y){
        printf("%d é maior que %d", x, y);
        return 1;
    }else if(x < y){
            printf("%d é maior que %d", y, x);
            return 2; }
                else
                printf("%d é igual a %d", x, y);
    return 0;
};
